.title {
  text-align: center;
}

.user_img {
  width: 50px;
  height: 50px;
  border-radius: 24px;
  margin: 5px 10px;
}

.userS {
  display: flex;
  padding-bottom: 10px;
  border-bottom: 1px solid #ccc;
}

.user-1 {
  display: flex;
}

.user-1 view, user-2 {
  height: 50px;
  line-height: 50px;
}

.user-2 {
  text-align: right;
  width: 50%;
  height: 50px;
  line-height: 50px;
}

.user_details {
  border: 1px solid #1989fa;
  width: 40%;
  text-align: center;
  height: 30px;
  line-height: 30px;
  border-radius: 4px;
  color: #1989fa;
  margin-top: 10px;
  float: right;
}

.Pop-up-layer{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6)
}
.Pop-up-content{
   position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 90%;
  height: 100px;
  margin: 34% auto;
  background: white
  
}